{
    "manifest_version": 3,
    "name": "Collectr Portfolio Exporter",
    "version": "1.0",
    "description": "Export your Collectr portfolio to a CSV file.",
    "permissions": ["scripting", "tabs", "activeTab"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "logo16.png",
        "48": "logo48.png",
        "128": "logo128.png"
      }
    },
    "icons": {
      "16": "logo16.png",
      "48": "logo48.png",
      "128": "logo128.png"
    },
    "host_permissions": [
      "https://*/*"
    ]
  }
  